    "fig, ax = plt.subplots(figsize = (16, 10))\n",
    "ax.vlines(x = games_count['year_of_release'], ymin = 0, ymax = games_count['name'], color = 'blue', alpha = 0.6, linewidth = 2)\n",
    "ax.scatter(x = games_count['year_of_release'], y = games_count['name'], s = 75, color = 'black', alpha = 0.6)\n",
    "ax.set_xticks(games_count['year_of_release'])\n",
    "ax.set_xticklabels(games_count['year_of_release'], rotation = 40)\n",
    "plt.title(\"Number of games rolled out over the years\")\n",
    "\n",
    "# this next trick is really all thanks to our dear instructor who showed it in a webinar. \n",
    "# this was hard to find elsewhere!\n",
    "\n",
    "for row in games_count.itertuples():\n",
    "    ax.text(row.year_of_release, row.name + 40, s = round(row.name, 2))\n",
    "\n",
    "plt.show()"
   ]
